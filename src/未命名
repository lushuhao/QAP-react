<!-- 业务处理 -->
<script type="text/javascript" src="//q.aiyongbao.com/trade/d20170515ayb/web/js/index/trade.js?qntag=2"></script>
<!-- 初始化载入 -->
<script>
$(function(){
    $('label[name=index_flag]').click(function(){
        if($(this).hasClass("checked")){
            $(this).removeClass("checked");
        }
        else{
            $(this).addClass("checked");
        }
    });

    $('.smsswitch').change(function(event) {
        var that = this;
        var smsswitchstatus = $(this).prop('checked');
        var targettype = $(this).attr('name');
        switch(targettype){
            case 'smspay':
                ittoolspam('TD20160224140258', 'WaitingMessagePayOpen');
                break;
            case 'smswl':
                ittoolspam('TD20160224140258', 'WaitMessageDelayOpen');
                break;
            case 'smssend':
                if($(this).attr('data-site') == 'send'){
                    ittoolspam('TD20160224140258', 'DetailsDeliverButtonClick');
                }else{
                    ittoolspam('TD20160224140258', 'GoodsMessageDetailOpen');
                }
                break;
            case 'smsgood':
                ittoolspam('TD20160224140258', 'WaitforCommentOpen');
                break;
        }
        var loadi = layer.load(5);
        QN.application.invoke({
            "cmd": "getLoginuser",
            "success": function (e) {
                if (typeof  e === 'string') {
                    e = JSON.parse(e);
                }
                if (e.sub_user_nick != null && e.sub_user_nick != '') {
                    layer.closeAll();
                    $.alert('<p>子账号无法操作，请联系主账号开启！</p>');
                    $(that).prop('checked', !smsswitchstatus);
                    return;
                }
                $.ajax({
                    dataType: 'jsonp',
                    url: APP_WEB_INDEX_ROOT + '/iytrade2/zdrate',
                    success: function (data) {
                        if (data == 'fail') {
                            layer.msg('亲，您本次登陆失效或操作超时，为了安全，请先关闭插件，重新打开再操作。', 3, 3);
                            $(that).prop('checked', !smsswitchstatus);
                            return;
                        }
                        if(data == 'off'){
                            layer.closeAll();
                            $.alert('<p>您的登陆已失效，请关闭插件重新进入</p>');
                            $(that).prop('checked', !smsswitchstatus);
                        }else{
                            if(smsswitchstatus){
                                var num = $('#smssum').text();
                                num = Number(num.substr(0, num.length - 1));
                                if(num == 0){
                                    $('#sms_a').trigger('click');
                                }
                                QN.top.invoke({
                                    /*应用为用户开通的增量消息服务*/
                                    "cmd": 'taobao.tmc.user.permit',
                                    "method": "POST",
                                    "success": function () {
                                        $.ajax({
                                            url: APP_WEB_INDEX_ROOT + '/iytrade2/switchoption',
                                            data: {
                                                type: targettype,
                                                smsoff: 'on'
                                            },
                                            dataType: 'jsonp',
                                            timeout: 10000,
                                            success: function (data) {
                                                if (data == 'fail') {
                                                    layer.msg('亲，您本次登陆失效或操作超时，为了安全，请先关闭插件，重新打开再操作。', 3, 3);
                                                    $(that).prop('checked', !smsswitchstatus);
                                                    return;
                                                }
                                                layer.closeAll();
                                                layer.msg('开启成功!', 2, 1);
                                            },
                                            error: function () {
                                                layer.closeAll();
                                                $.alert('<p>开启失败，请重试</p>');
                                                $(that).prop('checked', !smsswitchstatus);
                                            }
                                        });
                                    },
                                    "error": function (msg) {
                                        layer.closeAll();
                                        $.alert('<p>开通消息失败，请重试！</p>');
                                        $(that).prop('checked', !smsswitchstatus);
                                    }
                                });
                            }else{
                                $.ajax({
                                    url: APP_WEB_INDEX_ROOT + '/iytrade2/switchoption',
                                    data: {
                                        type: targettype,
                                        smsoff: 'off'
                                    },
                                    dataType: 'jsonp',
                                    timeout: 10000,
                                    success: function (data) {
                                        if (data == 'fail') {
                                            layer.msg('亲，您本次登陆失效或操作超时，为了安全，请先关闭插件，重新打开再操作。', 3, 3);
                                            $(that).prop('checked', !smsswitchstatus);
                                            return;
                                        }
                                        layer.closeAll();
                                        layer.msg('关闭成功!', 2, 1);
                                    },
                                    error: function () {
                                        layer.closeAll();
                                        $.alert('<p>关闭失败，请重试</p>');
                                        $(that).prop('checked', !smsswitchstatus);
                                    }
                                });
                            }
                        }
                    },
                    error: function () {
                        layer.closeAll();
                        $.alert('<p>授权信息获取失败，请重试!</p>');
                        $(that).prop('checked', !smsswitchstatus);
                    }
                });
            },
            error: function(){
                layer.closeAll();
                $.alert('<p>登陆信息获取失败，请重试!</p>');
                $(that).prop('checked', !smsswitchstatus);
            }
        });
    });

    if(navigator.userAgent.indexOf('Mac OS X')>-1){
      window.onload();
    }
});
/**
 * 刷新index
 * @author Duanhq
 */
var isreshow=false;
function reshow(){
    if(isreshow){
        return;
    }else{
        isreshow=true;
        getordersta(gsta,gpageno);
    }
}
/**
 * 页面载入后一系列初始化加载处理
 * @author Duanhq
 * @parm gtid,全局变量-主订单id
 * @parm gsta,全局变量-当前查看的订单所属状态
 * @parm sellertype,全局变量-卖家店铺类型（B，C店）
 * @parm buyernick,全局变量-买家昵称
 * @parm user_nick,全局变量-卖家昵称 from top.php
 * @parm vip_time,全局变量-订购到期时间
 * @parm vipuser,全局变量-身份信息
 * @parm version,全局变量-千牛版本号
 */
var APP_DB = 'https://trade.aiyongbao.com/Userscenters/';
var gtid = '';
var gsta = '';
var sellertype='';
var buyernick=null;
var version="";
var tsdzon='off';/*是否开启特殊地址*/
var tsdzval='';/*用户设置的特殊地址*/
var upWeight = 0;/*数据库中设置的重量*/
var upPrice=0;
var oneWeight = 0;/*单个重量*/
var pjxq='';/*保存评价信息*/
var layer_dialog_index=false;
var alltid=[];
var showadCount = 0;
window.onload = function(){
console.log('window.onload');
    ittoolspam('TD20160620190618','Afterpagedisplayed');
    setTimeout(checkAuth,2000);
    setTimeout(advert_black,2000);
    setTimeout(function(){
        becaonNew('ShowOldVersion','TD20160224140258');
        ittoolspam('TD20141112141143', '1008PCFirst');
    },2000);
    onload_viptime('index').done(function(){
        ittoolspam('TD20160620190618', 'Afterobtaininguserinformation');
        Authorization_judge();
        show_adv_top();
    });
    var huidiaosuccess = false;
    typeof QN.plugin.successResponse == 'function' ? QN.plugin.successResponse({}) : huidiaosuccess=true;
    setTimeout(function(){
        isEmpty(user_nick) ? refshvip_top() : console.log("%c"+user_nick,"font-size:20px;color:red");
    },2000);
    setTimeout(function(){
        onloadtimeinput();
        var addr_data = localStorage.getItem(user_nick+'Glset');
        if(isEmpty(addr_data)){/*加载特殊地址预警*/
            $.ajax({
                url:'https://trade.aiyongbao.com/iytrade2/getuserglsz',
                dataType:'jsonp',
                error:function(){},
                success:function(rsp){
                    if(rsp.tsdz){
                        if(rsp.tsdz.split(';')[0]=='on'){
                            tsdzon='on';
                            if(rsp.tsdz.split(';')[1]){tsdzval=rsp.tsdz.split(';')[1];}
                            localStorage.setItem(user_nick+'Glset',rsp.tsdz);
                        }
                    }else{
                        localStorage.setItem(user_nick+'Glset','off;');
                    }
                },
                error:function(msg){
                  console.log(msg);
                }
            });
        }else{
            if(addr_data.split(';')[0]=='on'){
                tsdzon='on';
                if(addr_data.split(';')[1]){tsdzval=addr_data.split(';')[1];}
            }
        }
        get_userinfo_data();
        var ishide = localStorage.getItem(user_nick+"_isunfold");
        if(!isEmpty(ishide)){
            ishide == 1 ? showhide('sq') : showhide('zk');/*展开显示高级搜索*/
        }
        localStorage.getItem("hidelocal") =="yes" ? $("#hideids").checkbox().checkbox('check') : null;/*显示和关闭隐藏的订单*/
    },200);
    if(huidiaosuccess){
        console.log('回调没有成功，继续回调'),huidiao();
    }

    layer.use('extend/layer.ext.js', function(){});
    html5sql.openDatabase( "db_item", "商品", 30 * 1024 * 1024 );
    initDataBase();/*初始化数据库*/
    QN.initTheme();
    $("#indexsel").attr("class","sui-dropdown active");
    $("#addtaskpl").show();
    $("#all_list").html('<div class="sui-loading loading-small " style="overflow: hidden;"><i class="sui-icon icon-pc-loading"></i></div>');

    /*判断来源*/
    var cometo = location.search.substr(1).split("&")[0];
    if(cometo.indexOf("cometo") > -1){
        if(cometo.split("=")[1] == "servicepromise"){
            setSystem();
        }
    }

    var shash=location.hash.substr(1);
    var tradestatus = null;
    if(!isEmpty(shash)){
        var params = shash.split('&');
        var map = new HashMap();
        for (var i in params) {
            var p = params[i].split("=");
            if(p.length == 2) {
                map.put(p[0], p[1]);
            }
        }
        vipuser = map.get("vipuser");/*身份信息*/
        user_nick = map.get('nick');
        gtid=map.get('tid');
        buyernick=map.get('buyerNick');
        tradestatus=map.get('tradeStatus');
        var event=map.get('event');
        var tid=map.get('tid');
        if(map.get("gsta")!=null){
            gsta=map.get("gsta");
            switch(gsta){
                case "dfh":$('#myTab a[href="#dfh"]').tab('show');break;
                case "dpj":$('#myTab a[href="#dpj"]').tab('show');break;
                case "dfk":$('#myTab a[href="#dfk"]').tab('show');break;
                case "yfh":$('#myTab a[href="#yfh"]').tab('show');break;
                case "tkz":$('#myTab a[href="#tkz"]').tab('show');break;
                case "sussce":$('#myTab a[href="#sussce"]').tab('show');break;
                case "close":$('#myTab a[href="#close"]').tab('show');break;
                default:$('#myTab a[href="#all"]').tab('show');
            };
        }/*疑问*/
        if(isEmpty(buyernick) && isEmpty(tradestatus) && isEmpty(gsta)){/*疑问*/
            if(event=='tradeDetail' && !isEmpty(tid)){
                var nickx=map.get('nick');
                if(!isEmpty(nickx)){/*得到用户名不为空*/
                    user_nick=nickx.split(':')[0];
                }
                todetails(tid,'task');
            }else{
                showlist();
            }
        }else if(gsta==""){
            if(buyernick!=null){
                buyernick=decodeURIComponent(buyernick);
                var buyernickarr =buyernick.split(":");
                buyernick=buyernickarr[0];
                $("#txt_select").val(buyernick);
                $("#txt_select_unfold").val(buyernick);
                sselect();
            }
            if(tradestatus==null){
                $("#sselectsta li").removeAttr('class');
                $("#sselectsta").siblings('a').children('input').val('all');
                $("#sselectsta").siblings('a').children('span').html('所有');
                $("#sselectsta li a[value='all']").parent('li').attr('class','active');
                if(event=='tradeDetail' && tid!=''){/*跳详情去*/
                    todetails(tid,'task');
                }
            }else{
                $("#sselectsta li").removeAttr('class');
                if(tradestatus=='WAIT_BUYER_PAY' || tradestatus=='no_create_pay'){/*代付款*/
                    tradestatus='wait_buyer_pay';
                }
                if(tradestatus=='WAIT_SELLER_SEND_GOODS'){/*代发货*/
                    tradestatus='wait_seller_send_goods';
                }
                if(tradestatus=='WAIT_BUYER_%20APPRAISE'||tradestatus=='BUYER_CONFORM_GOODS'||tradestatus=='WAIT_BUYER_APPRAISE'){/*待评价*/
                    tradestatus='buyer_conform_goods';
                }
                if(tradestatus=='TRADE_REFUND'){/*退款中*/
                    tradestatus='refunding';
                }
                switch(tradestatus){
                    case 'no_create_pay':$("#sselectsta").siblings('a').children('input').val('dfk');$("#sselectsta").siblings('a').children('span').html('待付款');$("#sselectsta li a[value='dfk']").parent('li').attr('class','active');break;
                    case 'wait_buyer_pay':$("#sselectsta").siblings('a').children('input').val('dfk');$("#sselectsta").siblings('a').children('span').html('待付款');$("#sselectsta li a[value='dfk']").parent('li').attr('class','active'); break;
                    case 'wait_seller_send_goods':$("#sselectsta").siblings('a').children('input').val('dfh');$("#sselectsta").siblings('a').children('span').html('待发货');$("#sselectsta li a[value='dfh']").parent('li').attr('class','active');break;
                    case 'buyer_conform_goods':$("#sselectsta").siblings('a').children('input').val('dpj');$("#sselectsta").siblings('a').children('span').html('待评价');$("#sselectsta li a[value='dpj']").parent('li').attr('class','active');break;
                    case 'refunding':$("#sselectsta").siblings('a').children('input').val('tkz');$("#sselectsta").siblings('a').children('span').html('退款中');$("#sselectsta li a[value='tkz']").parent('li').attr('class','active');break;
                    case 'trade_finish':$("#sselectsta").siblings('a').children('input').val('sussce');$("#sselectsta").siblings('a').children('span').html('已成功');$("#sselectsta li a[value='sussce']").parent('li').attr('class','active');break;
                    case 'trade_close':$("#sselectsta").siblings('a').children('input').val('close');$("#sselectsta").siblings('a').children('span').html('已关闭');$("#sselectsta li a[value='close']").parent('li').attr('class','active');break;
                    case 'trade_close_by_taobao':$("#sselectsta").siblings('a').children('input').val('close');$("#sselectsta").siblings('a').children('span').html('已关闭');$("#sselectsta li a[value='close']").parent('li').attr('class','active');break;
                    default :$("#sselectsta").siblings('a').children('input').val('all');$("#sselectsta").siblings('a').children('span').html('所有');$("#sselectsta li a[value='all']").parent('li').attr('class','active');break;
                }
                sselect();
            }
        }else{
            showlist();
        }
    }else{
        showlist();
    }

    /*个人中心6折优惠订购机会*/
    getiycoinFun().done(function(rsp) {
        var iynum = rsp.iycoin;
        var IsShowSixDisCount = localStorage.getItem("Is_ShowSixDiscount") || null;
        if( (iynum >= 1000) && (isEmpty(IsShowSixDisCount) || IsShowSixDisCount == undefined || IsShowSixDisCount == "undefined") ){
            var dometips = $("#PersonCenterTips");
            setTimeout(function(){
                localStorage.setItem("Is_ShowSixDiscount","Yes");
                /*layer.tips('<a style="cursor: pointer;text-decoration: none;color:#fff;" href="javascript:jumpPage(\'integral\');">签到领短信</a>', dometips, {
                    style: ['background-color:#78BA32; color:#fff', '#78BA32'],
                    maxWidth:185,
                    time: 5,
                    closeBtn:[0, true]
                })*/
            },3000);
        }
    });
    /*6折订购优惠结束*/
    setTimeout(function(){
        var alltid=[];
        $("#all table").each(function(){
            alltid.push($(this).prop("id").replace("table"+gsta,""));
        })
        getredis(alltid,"index");
        getautoAndcplj();/*加载时判断差评拦截，短信关怀，自动评价，中差评提醒是否开启*/
    },2000);
    ShouQiStatus();
    /*头部运营，注释
    setTimeout(showTopBusiness, 800);
    $('#togglebusiness').on('click', function(){
        if(window.localStorage.getItem(user_nick + 'hideBusiness')){
            window.localStorage.removeItem(user_nick + 'hideBusiness');
            $(this).html('隐藏');
            showTopBusiness();
        }else{
            window.localStorage.setItem(user_nick + 'hideBusiness', true);
            $(this).html('展示');
            $('#business>table').hide();
        }
        ittoolspam('TD20170111150123','20170111PCHidden');
    });*/

    /*showDifferentOrders();*//*初始化加载不同状态的订单数量 wtq 已废弃 by Shitou*/
    /*var newandoldtipsflag = window.localStorage.getItem('tradedialogxxxx');
    if(newandoldtipsflag!="20150328"){
