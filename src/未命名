        $('#mupdate>h3').hover(function() {
            if(vipuser==1){
                ittoolspam('TD20160415150440','1020ShowGO');
            }else{
                ittoolspam('TD20160415150440','1020ShowUpdate');
            }
            $('#buyVipModal').show();
        },function(){
            $('#buyVipModal').hide();
        });
        // 取消之前样式
        // $(".nav").slide({ type:"menu",  titCell:".m", targetCell:".sub", delayTime:0, triggerTime:0,returnDefault:true,defaultPlay:false  });
        $(".topNav").slide({ type:"menu",  titCell:"dd", targetCell:"ul", delayTime:0,defaultPlay:false,returnDefault:true  });

        $(window).on('load',function(event){
            event.preventDefault();
            var e = document.createEvent('HTMLEvents');
            e.initEvent( 'pageview', true, true ); /*冒泡 不终止默认行为*/
            document.body.dispatchEvent(e);
        });
        /* 派发ctr事件 */
        var ctrHandle = function() {
            var e = document.createEvent('HTMLEvents');
            e.initEvent( 'ctr', true, true ); /*冒泡 不终止默认行为*/
            this.dispatchEvent(e);
        };
        /* 功能点击统计 需要进行点击统计的在元素上增加data-ctr=事件编号即可  */
        $(document).on('click', '[data-ctr]:not([role=dialog])', ctrHandle);
        /* 弹层统计每个需要统计的弹层需要有role=dialog和data-ctr */
        $(document).on('shown', '[role=dialog]', ctrHandle);
        /* PV统计  body增加data-pv=事件编号 特殊需求自己扩展 如需要手动调用 将方法放开不要写成匿名的就好了*/
        $(document).on('pageview', 'body', function(){
            var pv  = $(this).data('pv'),   /* 事件编号 */
                eid = $(this).data('eid') || 'TD20141112141143',     /* 父级编号 */
                page= $(this).data('page') || false;


            setTimeout(function(){
                if(user_nick){
                    if(page && vipuser!=1){
                        beacon('p=TD20150323150341&n=' + user_nick + '&e=' + page);
                    }
                    if( (pv && eid) ) {
                        beacon &&
                        beacon('p=' + eid + '&n=' + user_nick + '&e=' + pv);
                    }
                }else{
                    QN.application.invoke({
                        'cmd' : 'getLoginuser',
                        'success' : function(rsp){
                            var user_nick = rsp.user_nick;
                            if(page && vipuser!=1){
                                beacon('p=TD20150323150341&n=' + user_nick + '&e=' + page);
                            }
                            if( (pv && eid) ) {
                                beacon &&
                                beacon('p=' + eid + '&n=' + user_nick + '&e=' + pv);
                            }
                        }
                    });
                }
            },1000);

        });
        /* CTR事件 */
        $(document).on('ctr', '[data-ctr]', function() {
            var ctr = $(this).data('ctr'),  /* 事件编号 */
                eid = $(this).data('eid') || 'TD20141112161150',   /* 父级编号 */
                clk = $(this).data('clk') || false;   /*是否陈龙科需求的埋点*/
            if(clk){
                beacon('p=TD20150323150341&n=' + user_nick + '&e=' + clk);
            }
            if( (ctr && eid) ) {   /* 如果获取到值就进行的统计 */
                beacon &&
                beacon('p=' + eid + '&n=' + user_nick + '&e=' + ctr);
            }
        });
    })(jQuery);
</script>
