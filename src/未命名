    <% if(it.smslog && typeof(it.smslog) === "object" && it.smslog.length> 0){ %>
        <% var type = ''; %>
        <% var mytid = ''; %>
        <% for(var i=0 ; i < it.smslog.length; i++){ %>
            <% if(it.smslog[i].smstype == 'smspay'){%>
                <% type = '催付提醒';%>
            <%}else if(it.smslog[i].smstype == 'smssecond'){%>
                <% type = '二次催付提醒';%>
            <%}else if(it.smslog[i].smstype == 'smssend'){%>
                <% type = '发货提醒';%>
            <%}else if(it.smslog[i].smstype == 'smstrade'){%>
                <% type = '手动发送';%>
            <%}else if(it.smslog[i].smstype == 'smsgood'){%>
                <% type = '评价感谢';%>
            <%}else if(it.smslog[i].smstype == 'smsbad'){%>
                <% type = '中差评提醒';%>
            <%}else if(it.smslog[i].smstype == 'smsdefen'){%>
                <% type = '差评拦截';%>
            <%}else if(it.smslog[i].smstype == 'smswl'){%>
                <% type = '收货提醒';%>
            <%}else if(it.smslog[i].smstype == 'smsbatch'){%>
                <% type = '群发短信';%>
            <%}%>
            <%  mytid = it.smslog[i].tid.split("#")[0]; %>
            <% if (it.smslog[i].smstype == 'smsbatch') { %>
                <% mytid = "群发短信";%>
                <% var dd = it.smslog[i].tid.split('#')[1];%>
                <% it.smslog[i].sendphone = '群发短信';%>
