<script type="text/javascript" src="//q.aiyongbao.com/trade/d20170515ayb/web/js/index/trade_tag.js?qntag=1"></script>
<script type="text/javascript">
    var resize = function () {
        $container = $(".container");
        $container.height($(window).height() - $('.topNav').height() - $('#daoh').height() - $('#daohfoots').height());
    };
    resize();
    $(window).resize(resize);
</script>
<script>
    /**
     * String    @parm user_nick 获取当前用户昵称:主账号
     * Datetime  @parm viptime   获取当前用户高级版到期时间:yyyy-mm-dd h-i-s
     * Datetime  @parm usertime  获取当前用户初級版到期时间:yyyy-mm-dd h-i-s
     * Int         @parm vipuser   获取当前用户版本信息:初级版-0；高级版-1
     * String    @parm version   获取当前产品信息：trade/item
     * String    @parm tmark     用戶標記
     **/
    function getConfig() {
        var data = {};
        data.user_nick = user_nick;
        data.viptime = viptime;
        data.usertime = viptime;
        data.vipuser = vipuser;
        data.version = 'trade';
        data.tmark = tmark;
        data.logincount = logincount;
        data.mau = mau;
        return data;
    }
    function showbanner() {
        var oHead = document.getElementsByTagName('HEAD').item(0);
        var oScript = document.createElement("script");
        oScript.type = "text/javascript";
        oScript.src = "//q.aiyongbao.com/yunying/mothpro.js?v=" + new Date().getTime();
        oHead.appendChild(oScript);
        oScript.onload = function () {
            var oHead = document.getElementsByTagName('HEAD').item(0);
            var oScript = document.createElement("script");
            oScript.type = "text/javascript";
            // oScript.src = "http://tradeallen.aiyongbao.com/app/views/public/loginad.js?v=" + new Date().getTime();
           /* oScript.src = "//q.aiyongbao.com/yunying/loginad.js?v=" + new Date().getTime();*/
            /*oScript.src = "http://cc.aiyongbao.com/app/views/public/loginad.js?v=" + new Date().getTime();*/
            oScript.src = "https://trade.aiyongbao.com/public/loginad.js?qntag=2" + new Date().getTime();
            oHead.appendChild(oScript);
            oScript.onload = function () {
                var loginad = new loginads(function () {
                    showbanners();
                });
                loginad.showloginad();
            }
        }
    }
    function closeadmsg() {
        login_spam('TD20150313170338', 'ClickResult');
        $('#showratead').modal('hide');
        $('#admsg').empty();
        window.location.href = APP_WEB_INDEX_ROOT_CDN + 'autoeva' + HTML_TYPE + "?qntag=2";
    }
</script>
