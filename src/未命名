                sellertype == "C" ? $("#fhpz_set").hide() : "";
            }

        },1000);*/
        // 短信充值 cbl
        $('#m1').mouseover(function(){
            // $('#nav_m1_ul').show();
            $('#smssum').text("充值");
            $('#sms_a').css("color","#fff");
            $('#sms_img').attr("src","//q.aiyongbao.com/trade/web/images/smsl_cbl.png?qntag=2");
        });
        $('#m1').mouseout(function(){
            // $('#nav_m1_ul').hide();
            $('#smssum').text($('#smscount').text()+"条");
            $('#sms_a').css("color","#333");
            $('#sms_img').attr("src","//q.aiyongbao.com/trade/web/images/smsd_cbl.png?qntag=2");
        });
        // 版本信息 cbl
        $('#m2').mouseover(function(){
            $('#nav_m2_ul').show();
            $('#version_a').css("color","#fff");
            $('#version_img').attr("src","//q.aiyongbao.com/trade/web/images/ppl_cbl.png?qntag=2");
