<script>
    function jumpyjdf() {
        ittoolspam('TD20160518150533','PCJY-yjdx');
        var url = 'http://q.aiyongbao.com/gx/html/gx_banner_page.html?user_nick=' + user_nick;
        window.open(url);
    }

    function showsmspopup(){
        ittoolspam('TD20160224140258','firstChargeBarClick');
        ittoolspam('TD20160224140258','firstChargeBuyWinShow');
        var imgurl = '//q.aiyongbao.com/trade/web/img/sms_firstchargepopup2.png';
        var htmldivstr ='<div style="text-align:center;cursor: pointer;">' +
            '<img src="' + imgurl + '" border="0" id="first_charge" usemap="#first_charge_map" />' +
            '<map name="first_charge_map" id="first_charge_map">' +
                '<area shape="rect" onfocus="this.blur();" coords="23,411,260,462" href="javascript:firstchargego(60);" />' +
                '<area shape="rect" onfocus="this.blur();" coords="280,411,476,462" href="javascript:firstchargego(2);" />' +
            '</map>' +
        '</div>';
        $.layer({
            type: 1,
            closeBtn: [0, true],
            shadeClose: false,
            border : [0],/* 边框*/
            shade: [0.8, '#333'],
            title: false,
            area: ['500px', '500px'],
            /*宽 ，高*/
            move: true,
            fix : true,
            /*不允许拖拽*/
            page: {
                html: htmldivstr
            },
        });
    }

    function firstchargego(price){
        if(price == 2){
            ittoolspam('TD20160224140258','firstChargeBuyWinClick2');
        }else{
            ittoolspam('TD20160224140258','firstChargeBuyWinClick60');
        }
        layer.closeAll();
        var loadi = layer.load(5);
        $.ajax({
            url: '//sms.aiyongbao.com/pay/smsfirstcharge?type=tradesms&nick=' + user_nick + '&price=' + price,
            type: 'GET',
        })
        .done(function(data) {
            layer.close(loadi);
            if (data == 'sql_error') {
                $.alert('<p>sql_error:订单生成失败，请重试！</p>');
            } else {
                window.localStorage.removeItem(user_nick + 'showfirstsmsad');
                $('#smsfirst').hide();
                QN.application.invoke({
                    cmd: "browserUrlEmbedded",
                    param: {"url": data, id: '1', "title": "短信订购", "width": '900', "height": '720'}
                });
                layer.confirm('完成付款了吗？', function () {
                    ittoolspam('TD20160224140258','firstChargeSuccessClick');
                    $.ajax({
                        url: APP_WEB_INDEX_ROOT + '/iytrade2/smsbuy',
                        dataType: 'jsonp',
                        success: function(rsp){
                            if(rsp.log.length == 0){
                                layer.close(layer.index);
                            }else{
                                chargeOpenWinshow();
                            }
                        },
                        error: function() {
                            layer.close(layer.index);
                        }
                    });
                },function(){
                    layer.close(layer.index);
                });
            }
        })
        .fail(function() {
            layer.close(loadi);
            $.alert('<p>操作失败，请重试！</p>');
        });
    }

    function chargeOpenWinshow(){
        ittoolspam('TD20160224140258','firstChargeOpenWinShow');
        layer.close(layer.index);
        var htmldivstr ='<div style="text-align:center;width: 400px;height: 380px;font-family: \'Arial Negreta\', \'Arial Normal\', \'Arial\';line-height: 32px;">' +
            '<div style="background-color: #f00;position: relative;padding: 16px;">' +
                '<div style="font-size: 28px;font-weight: 700;color: #fff;">开启自动发送，<br />一键召回老用户</div>' +
                '<div style="position: absolute; top: -8px; right: 5px;color: #ccc;cursor: pointer;font-size: 20px;" onclick="layer.closeAll();">×</div>' +
            '</div>' +
            '<div style="background-color: #fff;padding: 13px;">' +
                '<div style="text-align: left;margin-left: 60px;margin-right: 35px;">' +
                    '<div class="smsswitches"><input type="checkbox" checked="checked" />催付提醒：买家拍下10分钟-2小时未付款时发送</div>' +
                    '<div class="smsswitches"><input type="checkbox" checked="checked" />二次催付：买家拍下6小时-两天内未付款时发送</div>' +
                    '<div class="smsswitches"><input type="checkbox" checked="checked" />发货提醒：卖家点击发货时发送</div>' +
                    '<div class="smsswitches"><input type="checkbox" checked="checked" />收货提醒：买家已签收时发送</div>' +
                    '<div class="smsswitches"><input type="checkbox" checked="checked" />好评感谢：双方好评后发送</div>' +
                    '<div class="smsswitches"><input type="checkbox" checked="checked" />中差评提醒：双方互评后给中差评买家发送</div>' +
                '</div>' +
                '<div style="margin-top: 23px;"><span style="border: 2px solid #f00;padding: 9px 26px;color: #FF0000;font-size: 28px;cursor: pointer;" onclick="opensmscare();">开启</span></div>' +
            '</div>' +
        '</div>';
        $.layer({
            type: 1,
            closeBtn: 0,
            shadeClose: false,
            border : [0],/* 边框*/
            shade: [0.8, '#333'],
            title: false,
            area: ['400px', '380px'],
            /*宽 ，高*/
            move: true,
            fix : true,
            /*不允许拖拽*/
            page: {
                html: htmldivstr
            },
        });
        $('.smsswitches').find('input').each(function(index){
            $(this).click(function(event) {
                switch(index){
                    case 1:
                        ittoolspam('TD20160224140258','firstChargeOpenWinUrge2');
                        break;
                    case 2:
                        ittoolspam('TD20160224140258','firstChargeOpenWinDeliver');
                        break;
                    case 3:
                        ittoolspam('TD20160224140258','firstChargeOpenWinTake');
                        break;
                    case 4:
                        ittoolspam('TD20160224140258','firstChargeOpenWinGood');
                        break;
                    case 5:
                        ittoolspam('TD20160224140258','firstChargeOpenWinBad');
                        break;
                    default:
                        ittoolspam('TD20160224140258','firstChargeOpenWinUrge');
                        break;
                }
            });
        });
    }

    function opensmscare(){
        ittoolspam('TD20160224140258','firstChargeOpenWinOpen');
        var checkednum = 0;
        var smsarr = ['smspay', 'smssecond', 'smssend', 'smswl', 'smsgood', 'smsbad'];
        var checkedarr = [];
        $('.smsswitches').find('input').each(function(index) {
            if($(this).prop('checked')){
                checkednum++;
                checkedarr.push(index);
            }
        });
        if(checkednum == 0){
            return layer.closeAll();
        }
        QN.application.invoke({
            "cmd": "getLoginuser",
            "success": function (e) {
                if (typeof  e === 'string') {
                    e = JSON.parse(e);
                }
                if (e.sub_user_nick != null && e.sub_user_nick != '') {
                    layer.closeAll();
                    $.alert('<p>子账号无法操作，请联系主账号开启！</p>');
                    return;
                }
                $.ajax({
                    dataType: 'jsonp',
                    url: APP_WEB_INDEX_ROOT + '/iytrade2/zdrate',
                    success: function (data) {
                        if (data == 'fail') {
                            layer.closeAll();
                            layer.msg('亲，您本次登陆失效或操作超时，为了安全，请先关闭插件，重新打开再操作。', 3, 3);
                            return;
                        }
                        if(data == 'off'){
                            layer.closeAll();
                            $.alert('<p>您的登陆已失效，请关闭插件重新进入</p>');
                        }else{
                            QN.top.invoke({
                                /*应用为用户开通的增量消息服务*/
                                "cmd": 'taobao.tmc.user.permit',
                                "method": "POST",
                                "success": function () {
                                    if(checkednum == 6){
                                        $.ajax({
                                            url: APP_WEB_INDEX_ROOT + '/iytrade2/opensms',
                                            data: {
                                                smsoff: 'on'
                                            },
                                            dataType: 'jsonp',
                                            timeout: 10000,
                                            success: function (data) {
                                                layer.closeAll();
                                                if (data == 'fail') {
                                                    layer.msg('亲，您本次登陆失效或操作超时，为了安全，请先关闭插件，重新打开再操作。', 3, 3);
                                                    return;
                                                }
                                                layer.msg('开启成功!', 2, 1);
                                            },
                                            error: function () {
                                                layer.closeAll();
                                                $.alert('<p>开启失败，请重试！</p>');
                                            }
                                        });
                                    }else{
                                        for(var i = 0; i < checkednum; i++){
                                            $.ajax({
                                                url: APP_WEB_INDEX_ROOT + '/iytrade2/switchoption',
                                                data: {
                                                    type: smsarr[checkedarr[i]],
                                                    smsoff: 'on'
                                                },
                                                dataType: 'jsonp',
                                                timeout: 10000,
                                                success: function (data) {
                                                    if (data == 'fail') {
                                                        layer.msg('亲，您本次登陆失效或操作超时，为了安全，请先关闭插件，重新打开再操作。', 3, 3);
                                                        return;
                                                    }
                                                    layer.closeAll();
                                                },
                                                error: function () {
                                                    layer.closeAll();
                                                }
                                            });
                                        }
                                    }
                                },
                                "error": function (msg) {
                                    layer.closeAll();
                                    $.alert('<p>开通消息失败，请重试！</p>');
                                }
                            });
                        }
                    },
                    error: function () {
                        layer.closeAll();
                        $.alert('<p>授权信息获取失败，请重试!</p>');
                    }
                });
            }
        });
    }

    var APP_WEB_INDEX_ROOT = 'https://trade.aiyongbao.com';
    var APP_WEB_INDEX_ROOT_CDN = '//q.aiyongbao.com/trade/d20170515ayb/web/html/';
    var HTML_TYPE = '.html';
    ;(function($){
        $(document).on('click','a:contains("批量评价")',function(){
            ittoolspam('TD20141112141143','10085Comment');
            var isvip = this.dataset.novip || '';
            return isvip == "yes" ? checkVersion('batchrate') : location.href=APP_WEB_INDEX_ROOT_CDN+'batchEvaluation'+HTML_TYPE+'?qntag=99';
        });

        $("#systems").find('button:contains(重新授权)').bind('click',function(){
                $("#systems").modal('hide');
                $.alert(
                    {
                        backdrop: true,
                        title:'重新授权',
                        body:'确定重新授权？',
                        okBtn:'确定',
                        cancelBtn : '取消',
                        okHidden: function(e){
                            var url = 'https://oauth.taobao.com/authorize?response_type=code&client_id=21085840&redirect_uri=https://trade.aiyongbao.com/tc/trades2?scope=item&view=web&state=tradeindex';
                            QN.application.invoke({
                                cmd:'translateUrl',
                                param:{'url':url
                                },
                                success: function (e){
                                    if(typeof  e=== 'string'){
                                        e = JSON.parse(e);
                                    }
                                    location.href=e.url;
                                }
                            });
                        }
                    });
            }
        );
        $("#systems").find('button:contains(清除缓存)').bind('click',function(){
            $("#systems").modal('hide');
            $.alert({
                backdrop: true,
                title:'清除缓存',
                body:'确定清除缓存？',
                okBtn:'确定',
                cancelBtn : '取消',
                okHidden: function(e){
                    localStorage.clear();
                    QN.application.invoke({
                        cmd : 'refreshPluginSSO',
                        param : {},
                        error : function(msg, cmd, param) {
                            layer.msg("操作成功，请关闭插件再进",3,1);
                        },
                        success : function(rsp, cmd, param) {
                            layer.msg("操作成功，请关闭插件再进",3,1);
                        }
                    });
                }
            });
        });
        /**
         * 获取剩余短信条数
         * author：cbl
         */
        $.ajax({
            url: APP_WEB_INDEX_ROOT + '/iytrade2/sms',
            dataType: 'jsonp',
            timeout: 10000,
            success: function (datas) {
                if (datas == 'fail') {
                    layer.msg('亲，您本次登陆失效或操作超时，为了安全，请先关闭插件，重新打开再操作。', 3, 3);
                    return;
                }
                $('#smssum').text(datas.smsnum+"条");
                $('#smscount').text(datas.smsnum);
                if(datas.smsnum<20){
                    $('#smssum').css("color","red");
                }
            },
            error: function (datas) {
                $('#smssum').css("color","red");
                $('#smssum').text("获取失败");
            }
        });

        setTimeout(function(){
            var user = window.localStorage.getItem('loginnick') || user_nick;
            if(window.localStorage.getItem(user + 'showfirstsmsad')){
                ittoolspam('TD20160224140258','firstChargeBarShow2');
                $('#smsfirst').show();
                return;
            }
            if(window.localStorage.getItem(user + 'smsbuyyet')){
                $('#smsfirst').remove();
                return;
            }
            $.ajax({
                url: APP_WEB_INDEX_ROOT + '/iytrade2/smsbuy',
                dataType: 'jsonp',
                success: function(rsp){
                    if(rsp.log.length == 0){
                        $('#smsfirst').show();
                        ittoolspam('TD20160224140258','firstChargeBarShow');
                        ittoolspam('TD20160224140258','firstChargeBarShow2');
                        window.localStorage.setItem(user + 'showfirstsmsad', true);
                        window.localStorage.setItem(user + 'buymostsmstype', 0);
                    }else{
                        window.localStorage.setItem(user + 'smsbuyyet', true);
                        $('#smsfirst').remove();
                        var arr = [0, 0, 0, 0, 0];
                        for(var i = rsp.log.length - 1; i > -1; i--){
                            switch(true){
                                case rsp.log[i].price < 22:
                                    arr[0]++;
                                    break;
                                case rsp.log[i].price < 97:
                                    arr[1]++;
                                    break;
                                case rsp.log[i].price < 301:
                                    arr[2]++;
                                    break;
                                case rsp.log[i].price < 501:
                                    arr[3]++;
                                    break;
                                default:
                                    arr[4]++;
                                    break;
                            }
                        }
                        var max = [0, 0];
                        for(i = 0; i < 5; i++){
                            max = arr[i] > max[0] ? [arr[i], i] : max;
                        }
                        window.localStorage.setItem(user_nick + 'buymostsmstype', max[1]);
                    }
                },
                error: function() {
                }
            });
        }, 1000);

        // setTimeout(function(){
        //     var mydata = getConfig();
        //     console.log(mydata);
        //     $('#userid').text(mydata.user_nick);
        //     if (mydata.vipuser == 1) {
        //         $('#versioninfo').text("高级版");
        //         $('#version_span').text("高级版");
        //     }else{
        //         $('#versioninfo').text("初级版");
        //         $('#version_span').text("免费版");
        //     }
        //     var time = new Date(mydata.viptime).format('yyyy-MM-dd')
        //     $('#viptime').text(time);
        //     var day = getTimeDiff(time);

        //     },2000);



        $('#mfb2').hide();
        $('#mfb1').parents('ul').mouseover(function(){

            $('#mfb1').hide();
            $('#mfb2').show();
        });
        $('#mfb2').mouseout(function(){
            $('#mfb2').hide();
            $('#mfb1').show();
        });
        /*setTimeout(function(){
            if(user_nick == "x你我不散场" || user_nick=="weiye021"){

