}
/**
 *  初始化成交时间：用于选择交易订单
 *  @author Duanhq
 **/
 var ginput_end_date;
 var ginput_start_date;
function onloadtimeinput(){
    var date = new Date(serviceTime);
    ginput_end_date   = date.format('yyyy-MM-dd');
    date.setDate(date.getDate()-90);
    ginput_start_date = date.format('yyyy-MM-dd');
    $("#startime").val(ginput_start_date);
    $("#endtime").val(ginput_end_date);
    $('#date.input-daterange').datepicker({
        dateFormat:'yy-mm-dd',
        changeMonth: true,
        changeYear: true,
        startDate: ginput_start_date,/*d.getFullYear() + "-" + (d.getMonth() + 1) + "-" + d.getDate(),可选的最早时间*/
        endDate: ginput_end_date,/*可选的最远时间*/
        beforeShowDay: function (date){
          if (date.getMonth() == (new Date()).getMonth())
            switch (date.getDate()){
              case 4:
                return {
                  tooltip: 'Example tooltip'
                };
              case 12:
                return "green";
            }
        }
    }).on('changeMonth',function(e){

    });
}
</script>

