                    <% type = '首页好评群发';%>
                    <% it.smslog[i].sendphone = '首页好评群发';%>
                <% }%>
            <% }%>
            <tr>
                <td>
                    <%= it.smslog[i].smstime?it.smslog[i].smstime:"" %>
                </td>
                <td>
                    <%= mytid ? mytid:"" %>
                </td>
                <td>
                    <% if(it.smslog[i].status == "0"){%>
                        无提交
